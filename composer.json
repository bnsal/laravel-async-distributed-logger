{
    "name": "bnsal/laravel-async-distributed-logger",
    "description": "Laravel package to enable async distributed logging for your application",
    "keywords": [
        "Bnsal",
        "logging",
        "distributed logging",
        "logging on queue",
        "logging on redis",
        "async logging",
        "logging by redis",
        "logging by database",
        "logging by sqs"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Bnsal\\DistributedLogger\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Keshav Bansal",
            "email": "keshav@bnsal.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2|^8.0",
        "laravel/framework": "^6.0|^7.0|^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bnsal\\DistributedLogger\\DistributedLoggingProvider"
            ]
        }
    }
}
